    <validationRules>
        <fullName>Close_Date_Rule</fullName>
        <active>true</active>
        <description>If the Status of the position is changed to &quot;Closed-Filled&quot; or &quot;Closed-Cancelled&quot; or &quot;Closed-Cancelled&quot;, make sure that the close date is filled.</description>
        <errorConditionFormula>(ISPICKVAL( Status__c , &quot;Closed-Filled&quot;) ||
 ISPICKVAL( Status__c , &quot;Closed-Cancelled&quot;)||
 ISPICKVAL( Status__c , &quot;Closed-Not Approved&quot;))
 &amp;&amp; 
 ISBLANK( Close_Date__c )</errorConditionFormula>
        <errorDisplayField>Close_Date__c</errorDisplayField>
        <errorMessage>Please fill in the close date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_Pay_Rule</fullName>
        <active>true</active>
        <description>Prevent the user from saving a record if the maximum pay is less than the minimum pay.</description>
        <errorConditionFormula>Minimum_Pay__c &gt; Maximum_Pay__c</errorConditionFormula>
        <errorDisplayField>Maximum_Pay__c</errorDisplayField>
        <errorMessage>Please enter a value greater than or equal to Minimum Pay</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Pay_Rule</fullName>
        <active>true</active>
        <description>Prevent the user from saving a record if the min pay is less than 30,000</description>
        <errorConditionFormula>Minimum_Pay__c &lt; 30000</errorConditionFormula>
        <errorDisplayField>Minimum_Pay__c</errorDisplayField>
        <errorMessage>Please enter a value greater than or equal to 30000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_for_Open_Date</fullName>
        <active>true</active>
        <description>The open date should be the current date or any date in the future</description>
        <errorConditionFormula>Open_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Open_Date__c</errorDisplayField>
        <errorMessage>Please enter todays date or any date in the future</errorMessage>
    </validationRules>
